{
  "extends": ["airbnb", "plugin:jest/recommended"],
  "parser": "babel-eslint",
  "plugins": ["react-native", "flowtype", "jest"],
  "globals": {
    "fetch": true
  },
  "env": {
    "jest": true
  },
  "rules": {
    "semi": ["error", "never"],
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
  }
}
